{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.ts",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "mysql2": "^3.6.5",
    "node-fetch": "^2.6.7",
    "ts-node": "^10.2.1",
    "uuid": "^9.0.1"
  },
  "scripts": {
    "test": "jest --silent",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "start": "npm run build && node dist/src/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.1",
    "@types/node-fetch": "^2.6.9",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "nodemon": "^2.0.13",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  }
}
